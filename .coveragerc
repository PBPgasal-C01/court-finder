[run]
branch = True
source =
    blog
omit =
    */migrations/*
    */tests.py
    */env/*
    manage.py

[report]
show_missing = True
skip_covered = False
precision = 1
exclude_lines =
    pragma: no cover
    if __name__ == '__main__':
    raise NotImplementedError

[html]
directory = htmlcov
title = Court Finder Coverage
